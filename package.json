{
  "name": "Jeffry.in",
  "description": "Jeffry.in, Jeff Wainwright's blog",
  "version": "2.1.2",
  "author": "Jeff Wainwright <yowainwright@gmail.com> (jeffry.in)",
  "bugs": {
    "url": "https://github.com/yowainwright/yowainwright.github.io/issues"
  },
  "dependencies": {
    "@babel/core": "^7.13.14",
    "core-js": "^3.10.1",
    "gatsby": "^2.32.12",
    "gatsby-codemods": "2.3.0",
    "gatsby-link": "2.11.0",
    "gatsby-plugin-google-analytics": "3.3.0",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-offline": "3.10.2",
    "gatsby-plugin-preact": "3.3.0",
    "gatsby-plugin-prettier-eslint": "^1.0.4",
    "gatsby-plugin-react-helmet": "4.4.0",
    "gatsby-plugin-sass": "2.8.0",
    "gatsby-plugin-sharp": "2.14.3",
    "gatsby-plugin-typescript": "2.12.1",
    "gatsby-plugin-typography": "2.12.0",
    "gatsby-remark-copy-linked-files": "4.0.0",
    "gatsby-remark-images": "3.11.1",
    "gatsby-remark-prismjs": "3.13.0",
    "gatsby-remark-responsive-iframe": "2.11.0",
    "gatsby-remark-smartypants": "2.10.0",
    "gatsby-source-filesystem": "2.11.1",
    "gatsby-transformer-remark": "2.16.1",
    "gatsby-transformer-sharp": "2.12.1",
    "node-sass": "4.14.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.2",
    "react-typography": "0.16.19",
    "styled-components": "5.2.3",
    "typography": "0.16.19",
    "webpack": "5.33.2"
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx-experimental": "^7.12.11",
    "@babel/helper-plugin-utils": "7.13.0",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-optional-chaining": "7.13.12",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.13.15",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-react": "7.13.13",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@commitlint/prompt": "12.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-macros": "3.0.1",
    "commitizen": "4.2.3",
    "conventional-changelog-cli": "2.1.1",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "gatsby-cli": "3.3.0",
    "gatsby-plugin-feed": "2.13.1",
    "gh-pages": "3.0.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "markdown-spellcheck": "1.3.1",
    "prettier": "2.2.1",
    "prismjs": "1.23.0",
    "react-helmet": "6.1.0",
    "regenerator-runtime": "0.13.7",
    "stylelint": "13.12.0",
    "stylelint-config-dollarshaveclub": "8.0.0",
    "typescript": "4.2.4",
    "write-good": "1.0.8"
  },
  "homepage": "https://github.com/yowainwright/yowainwright.github.io#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/yowainwright.github.io"
  },
  "scripts": {
    "build": "gatsby build",
    "deploy": "yarn pre-deploy && NODE_ENV=production gatsby build && gh-pages -d public --branch master && yarn post-deploy",
    "dev": "gatsby develop",
    "develop": "GATSBY_GRAPHQL_IDE=playground NODE_ENV=development gatsby develop",
    "eslint": "eslint . --ext .js,.jsx --fix",
    "eslint:ci": "eslint --ext .js,.jsx",
    "grammar": "write-good src/pages/*.md --no-passive",
    "lint": "yarn eslint",
    "spelling": "mdspell '**/*.md' '!**/node_modules/**/*.md' --ignore-numbers",
    "spelling:ci": "mdspell '**/*.md' '!**/node_modules/**/*.md' --ignore-numbers --report",
    "start": "yarn && yarn develop",
    "pre-deploy": "cp CNAME public; cp *.html public; cp README.md public && cp -rf ./assets ./public",
    "post-deploy": "git tag $npm_package_version && git push origin --tags"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        120
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS =~ 1$ ]]; then yarn; fi",
      "post-merge": "yarn",
      "post-rewrite": "yarn",
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "yarn lint"
    ]
  }
}
