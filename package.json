{
  "name": "Jeffry.in",
  "description": "Jeffry.in, Jeff Wainwright's blog",
  "version": "2.6.4",
  "author": "Jeff Wainwright <yowainwright@gmail.com> (jeffry.in)",
  "bugs": {
    "url": "https://github.com/yowainwright/yowainwright.github.io/issues"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "core-js": "^3.19.3",
    "gatsby": "^3.14.6",
    "gatsby-codemods": "3.3.0",
    "gatsby-link": "2.11.0",
    "gatsby-plugin-google-analytics": "3.14.0",
    "gatsby-plugin-manifest": "3.14.0",
    "gatsby-plugin-offline": "4.14.0",
    "gatsby-plugin-preact": "3.3.0",
    "gatsby-plugin-prettier-eslint": "^1.0.6",
    "gatsby-plugin-react-helmet": "4.14.0",
    "gatsby-plugin-sass": "4.14.0",
    "gatsby-plugin-sharp": "2.14.4",
    "gatsby-plugin-typescript": "3.14.0",
    "gatsby-plugin-typography": "3.14.0",
    "gatsby-remark-autolink-headers": "^4.4.1",
    "gatsby-remark-copy-linked-files": "5.3.0",
    "gatsby-remark-images": "3.11.1",
    "gatsby-remark-prismjs": "5.11.0",
    "gatsby-remark-responsive-iframe": "4.11.0",
    "gatsby-remark-smartypants": "4.11.0",
    "gatsby-source-filesystem": "3.14.0",
    "gatsby-transformer-remark": "4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.4.2",
    "react-typography": "0.16.20",
    "sass": "1.44.0",
    "sharp": "^0.29.3",
    "styled-components": "5.3.3",
    "typography": "0.16.21",
    "webpack": "5.65.0"
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx-experimental": "^7.12.11",
    "@babel/helper-plugin-utils": "7.14.5",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-optional-chaining": "7.16.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@commitlint/prompt": "15.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.4.2",
    "babel-loader": "8.2.3",
    "babel-plugin-macros": "3.1.0",
    "commitizen": "4.2.4",
    "conventional-changelog-cli": "2.1.1",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.27.1",
    "gatsby-cli": "4.3.0",
    "gatsby-plugin-feed": "3.14.0",
    "gh-pages": "3.2.3",
    "husky": "^7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "markdown-spellcheck": "1.3.1",
    "prettier": "2.5.1",
    "prismjs": "1.25.0",
    "react-helmet": "6.1.0",
    "regenerator-runtime": "0.13.9",
    "stylelint": "14.1.0",
    "stylelint-config-dollarshaveclub": "8.0.0",
    "typescript": "4.5.2",
    "write-good": "1.0.8"
  },
  "homepage": "https://github.com/yowainwright/yowainwright.github.io#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/yowainwright.github.io"
  },
  "scripts": {
    "build": "gatsby build",
    "deploy": "yarn pre-deploy && NODE_ENV=production gatsby build && gh-pages -d public --branch master && yarn post-deploy",
    "dev": "gatsby develop",
    "develop": "GATSBY_GRAPHQL_IDE=playground NODE_ENV=development gatsby develop",
    "eslint": "eslint . --ext .js,.jsx --fix",
    "eslint:ci": "eslint --ext .js,.jsx",
    "grammar": "write-good src/pages/*.md --no-passive",
    "lint": "yarn eslint",
    "spelling": "mdspell '**/*.md' '!**/node_modules/**/*.md' --ignore-numbers",
    "spelling:ci": "mdspell '**/*.md' '!**/node_modules/**/*.md' --ignore-numbers --report",
    "start": "yarn && yarn develop",
    "pre-deploy": "cp CNAME public; cp *.html public; cp README.md public && cp -rf ./assets ./public",
    "post-deploy": "git tag $npm_package_version && git push origin --tags",
    "prepare": "husky install",
    "test": "yarn lint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        120
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS =~ 1$ ]]; then yarn; fi",
      "post-merge": "yarn",
      "post-rewrite": "yarn",
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "yarn lint"
    ]
  }
}
