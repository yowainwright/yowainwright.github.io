{
    "name": "Jeffry.in",
  "description": "Jeffry.in, Jeff Wainwright's blog",
  "version": "1.21.0",
  "author": "Jeff Wainwright <yowainwright@gmail.com> (jeffry.in)",
  "bugs": {
    "url": "https://github.com/yowainwright/yowainwright.github.io/issues"
  },
  "dependencies": {
    "@babel/core": "^7.12.17",
    "core-js": "^3.9.0",
    "gatsby": "^2.32.4",
    "gatsby-codemods": "1.10.0",
    "gatsby-link": "2.11.0",
    "gatsby-plugin-google-analytics": "2.11.0",
    "gatsby-plugin-manifest": "2.12.0",
    "gatsby-plugin-offline": "3.10.1",
    "gatsby-plugin-preact": "3.3.0",
    "gatsby-plugin-react-helmet": "3.10.0",
    "gatsby-plugin-sass": "2.8.0",
    "gatsby-plugin-sharp": "2.14.1",
    "gatsby-plugin-typescript": "2.12.0",
    "gatsby-plugin-typography": "2.12.0",
    "gatsby-remark-copy-linked-files": "2.10.0",
    "gatsby-remark-images": "3.11.0",
    "gatsby-remark-prismjs": "3.13.0",
    "gatsby-remark-responsive-iframe": "2.11.0",
    "gatsby-remark-smartypants": "2.10.0",
    "gatsby-source-filesystem": "2.11.0",
    "gatsby-transformer-remark": "2.16.0",
    "gatsby-transformer-sharp": "2.12.0",
    "node-sass": "4.14.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-dropzone": "^11.3.1",
    "react-final-form": "^6.5.2",
    "react-typography": "0.16.19",
    "styled-components": "5.2.1",
    "typography": "0.16.19",
    "webpack": "5.24.0"
  },
  "devDependencies": {
    "@babel/helper-plugin-utils": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-optional-chaining": "7.13.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.17",
    "@babel/preset-env": "7.12.16",
    "@babel/preset-react": "7.12.13",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "11.0.0",
    "@commitlint/prompt": "12.0.0",
    "@heartly/eslint-config": "1.0.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-macros": "3.0.1",
    "commitizen": "4.2.3",
    "conventional-changelog-cli": "2.1.1",
    "eslint": "^7.20.0",
    "gatsby-cli": "2.19.1",
    "gatsby-plugin-feed": "2.13.0",
    "gh-pages": "3.0.0",
    "husky": "5.1.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "markdown-spellcheck": "1.3.1",
    "markdownlint-cli": "0.26.0",
    "prettier": "2.2.1",
    "prismjs": "1.23.0",
    "prop-types": "15.7.2",
    "react-helmet": "6.1.0",
    "react-responsive-grid": "0.3.4",
    "react-test-renderer": "16.14.0",
    "regenerator-runtime": "0.13.7",
    "stylelint": "13.11.0",
    "stylelint-config-dollarshaveclub": "8.0.0",
    "typescript": "4.1.5",
    "write-good": "1.0.8"
  },
  "homepage": "https://github.com/yowainwright/yowainwright.github.io#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/yowainwright.github.io"
  },
  "scripts": {
    "build": "gatsby build",
    "chore:delete-branch": "if git show-ref --quiet refs/heads/chore-changelog; then git branch -D chore-changelog; fi",
    "chore:delete-changelog-branch": "if git show-ref --quiet refs/heads/chore-changelog; then git branch -D chore-changelog; fi",
    "chore:branch": "git checkout -b chore-changelog",
    "chore:changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0",
    "chore:setup-next-work": "git checkout develop && yarn chore:delete-changelog-branch",
    "chore:pr": "git add . && git commit -m '[chore] updates changelog' --no-verify && git push origin chore-changelog -f",
    "chore:setup-changelog": "git checkout develop && git pull",
    "chore": "yarn chore:delete-changelog-branch && yarn chore:branch && yarn chore:changelog && yarn chore:pr && yarn chore:setup-next-work",
    "deploy": "yarn pre-deploy && NODE_ENV=production gatsby build && gh-pages -d public --branch master && yarn post-deploy",
    "dev": "gatsby develop",
    "develop": "GATSBY_GRAPHQL_IDE=playground NODE_ENV=development gatsby develop",
    "eslint": "eslint . --ext .js,.jsx --fix",
    "eslint:ci": "eslint --ext .js,.jsx",
    "grammar": "write-good src/pages/*.md --no-passive",
    "lint": "yarn eslint",
    "markdownlint": "markdownlint *.md",
    "spelling": "mdspell '**/*.md' '!**/node_modules/**/*.md' --ignore-numbers",
    "spelling:ci": "mdspell '**/*.md' '!**/node_modules/**/*.md' --ignore-numbers --report",
    "start": "yarn && yarn develop",
    "test": "jest",
    "pre-deploy": "cp CNAME public; cp *.html public; cp README.md public && cp -rf ./assets ./public",
    "post-deploy": "git tag $npm_package_version && git push origin --tags && yarn chore"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "__snapshots__"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        120
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS =~ 1$ ]]; then yarn; fi",
      "post-merge": "yarn",
      "post-rewrite": "yarn",
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "yarn lint"
    ],
    "*.{ts,tsx}": [
      "jest"
    ]
  }
}
