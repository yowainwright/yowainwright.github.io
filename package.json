    "mitt": "^3.0.0",
    "platform": "^1.3.6",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^14.2.1",
    "react-typography": "0.16.20",
    "sass": "1.53.0",
    "shallow-compare": "^1.2.2",
    "sharp": "^0.30.6",
    "stack-trace": "1.0.0-pre1",
    "styled-components": "5.3.5",
    "typography": "0.16.21",
    "webpack": "5.73.0"
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx-experimental": "^7.12.11",
    "@babel/helper-plugin-utils": "7.17.12",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-proposal-optional-chaining": "7.18.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.18.5",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.3",
    "@commitlint/prompt": "17.0.0",
    "@giscus/react": "^2.0.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "28.1.2",
    "babel-loader": "8.2.5",
    "babel-plugin-macros": "3.1.0",
    "commitizen": "4.2.4",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.30.1",
    "gatsby-cli": "4.18.0",
    "gatsby-plugin-feed": "4.18.0",
    "gh-pages": "4.0.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "3.0.0",
    "lint-staged": "^13.0.1",
    "markdown-spellcheck": "1.3.1",
    "prettier": "2.7.1",
    "prismjs": "1.28.0",
    "react-helmet": "6.1.0",
    "regenerator-runtime": "0.13.9",
    "stylelint": "14.9.1",
    "stylelint-config-dollarshaveclub": "8.0.0",
    "write-good": "1.0.8"
  },
  "homepage": "https://github.com/yowainwright/yowainwright.github.io#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/yowainwright.github.io"
  },
  "scripts": {
    "build": "gatsby build",
    "deploy": "pnpm pre-deploy && NODE_ENV=production gatsby build && gh-pages -d public --branch master && pnpm post-deploy",
    "dev": "gatsby develop",
    "develop": "GATSBY_GRAPHQL_IDE=playground NODE_ENV=development gatsby develop",
    "eslint": "eslint ./src --ext .js,.jsx --fix",
    "eslint:ci": "eslint ./src --ext .js,.jsx",
    "grammar": "write-good src/pages/*.md --no-passive",
    "lint": "pnpm eslint:ci",
    "spelling": "mdspell '**/*.md' '!**/node_modules/**/*.md' --ignore-numbers",
    "spelling:ci": "mdspell '**/*.md' '!**/node_modules/**/*.md' --ignore-numbers --report",
    "start": "pnpm install && pnpm develop",
    "pre-deploy": "cp CNAME public; cp *.html public; cp README.md public && cp -rf ./assets ./public",
    "post-deploy": "git tag $npm_package_version && git push origin --tags",
    "prepare": "husky install",
    "test": "pnpm lint"
  },
  "resolutions": {
    "ansi-html": "^0.0.8",
    "ansi-regex": "^3.0.1",
    "async": "^3.2.4",
    "cross-fetch": "^3.1.5",
    "minimist": "^1.2.6",
    "multer": "1.4.5-lts.1",
    "postcss": "^8.2.13"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        120
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS =~ 1$ ]]; then yarn; fi",
      "post-merge": "pnpm",
      "post-rewrite": "pnpm",
      "pre-commit": "pnpm lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "pnpm lint"
    ]
  }
}
