{
  "name": "Jeffry.in",
  "description": "Jeffry.in, Jeff Wainwright's blog",
  "version": "1.0.0",
  "author": "Jeff Wainwright <yowainwright@gmail.com> (jeffry.in)",
  "bugs": {
    "url": "https://github.com/yowainwright/yowainwright.github.io/issues"
  },
  "dependencies": {
    "eslint": "^4.5.0",
    "gatsby": "^1.9.270",
    "gatsby-link": "^1.6.8",
    "gatsby-plugin-google-analytics": "^1.0.4",
    "gatsby-plugin-manifest": "^1.0.5",
    "gatsby-plugin-offline": "^1.0.4",
    "gatsby-plugin-preact": "^1.0.3",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sharp": "^1.6.47",
    "gatsby-plugin-typography": "^1.7.6",
    "gatsby-remark-copy-linked-files": "^1.5.2",
    "gatsby-remark-images": "^1.5.66",
    "gatsby-remark-prismjs": "^2.0.1",
    "gatsby-remark-responsive-iframe": "^1.4.3",
    "gatsby-remark-smartypants": "^1.4.3",
    "gatsby-source-filesystem": "^1.5.38",
    "gatsby-transformer-remark": "^1.7.2",
    "gatsby-transformer-sharp": "^1.6.1",
    "lodash": "^4.17.4",
    "react-responsive-grid": "^0.3.4",
    "redux-devtools-extension": "^2.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-jest": "^23.2.0",
    "babel-preset-env": "^1.6.1",
    "enzyme": "3.4.2",
    "enzyme-adapter-preact": "0.2.0",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint-plugin-react": "^7.3.0",
    "gh-pages": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "23.2.0",
    "prettier": "^1.5.3",
    "prismjs": "^1.8.1",
    "prop-types": "^15.6.0",
    "react-helmet": "^5.2.0"
  },
  "homepage": "https://github.com/yowainwright/yowainwright.github.io#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/yowainwright.github.io"
  },
  "scripts": {
    "dev": "gatsby develop",
    "eslint": "eslint . --ext .js,.jsx --fix",
    "eslint:ci": "eslint --ext .js,.jsx",
    "test": "jest --no-cache",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "pre-deploy": "cp CNAME public; cp *.html public; cp README.md public && cp -rf ./assets ./public",
    "deploy": "npm run pre-deploy && gatsby build && gh-pages -d public --branch master",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "jest": {
    "testURL": "https://localhost/",
    "transform": {
      ".(js|jsx)": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/",
      "/src/styles/"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ]
  }
}
