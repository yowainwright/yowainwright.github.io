source 'https://rubygems.org'
gem 'jekyll'

require 'json'
require 'open-uri'
versions = JSON.parse(open('https://pages.github.com/versions.json').read)

gem 'github-pages', github: "github/pages-gem", group: :jekyll_plugins
gem 'jekyll-sitemap'
gem 'jekyll-redirect-from'
gem 'jekyll-minifier'
gem 'jekyll-autoprefixer'
gem 'jemoji'
